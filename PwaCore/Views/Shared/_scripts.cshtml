<script>
    $("#autocomplete").fadeOut();
    $("#searchIcon").click(function() {
        $("#autocomplete").animate({
                width: ["toggle", "swing"],
                opacity: "toggle"
            },
            500, "linear",
            function() {});
    });

   

    //res = fetch('/home/search?input=لب')
    //    .then(response => {
    //        var a = response.json();
    //        console.log(a);
    //        return a;
    //    });
        //.then(console.log(response));
    $("#autocomplete").autocomplete({
        //minLength: 3,
        source: function (request, response) {
            var search = $("#autocomplete").val();
            $.getJSON("/home/search?input=" + search,
                function (data) {
                    //console.log(data);
                    response(data);
                });
        }
        , 
        select: function (a, b) {

            console.log(b.item.data);
            location.href = `/home/ShopSingle?productId=${b.item.data}`;
        }
    });

    
</script>

